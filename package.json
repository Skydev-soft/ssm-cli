{
	"name": "ssm-cli",
	"version": "1.0.8",
	"description": "SSM CLI",
	"author": "TriThuc2321 <trithuc23232@gmail.com>",
	"type": "module",
	"keywords": [
		"cli",
		"npmtool"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Skydev-soft/ssm-cli.git"
	},
	"homepage": "https://github.com/Skydev-soft/ssm-cli#readme",
	"license": "ISC",
	"bin": {
		"ssm-cli": "dist/index.js"
	},
	"main": "dist/index.js",
	"engines": {
		"node": ">=14.16"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup src/index.ts --format esm --clean --minify --metafile",
		"dev": "tsup src/index.ts --format esm --watch --clean --onSuccess \"node dist/index.js\"",
		"test": "vitest",
		"prepare": "husky install",
		"changeset": "changeset",
		"release": "changeset publish",
		"deploy": "git push github main"
	},
	"dependencies": {
		"axios": "^1.7.7",
		"chalk": "^5.2.0",
		"commander": "^9.5.0",
		"dotenv": "^16.4.5",
		"figlet": "^1.5.2",
		"open": "^10.1.0",
		"ora": "^6.1.2",
		"uuid": "^10.0.0"
	},
	"devDependencies": {
		"@changesets/cli": "^2.26.0",
		"@commitlint/cli": "^17.4.1",
		"@commitlint/config-conventional": "^17.4.0",
		"@trivago/prettier-plugin-sort-imports": "^4.0.0",
		"@types/figlet": "^1.5.5",
		"@types/node": "^18.11.18",
		"@types/uuid": "^10.0.0",
		"@typescript-eslint/eslint-plugin": "^5.48.1",
		"@typescript-eslint/parser": "^5.48.1",
		"eslint": "^8.31.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-prettier": "^4.2.1",
		"husky": "^8.0.3",
		"lint-staged": "^13.1.0",
		"prettier": "^2.8.2",
		"tsup": "^6.5.0",
		"type-fest": "^3.5.1",
		"typescript": "^4.9.4",
		"vitest": "^2.0.5"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": [
			"eslint --fix",
			"prettier --write"
		],
		"*.{md,mdx,yml,json}": [
			"prettier --write"
		]
	}
}
